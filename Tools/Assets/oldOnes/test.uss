.header {
    color: rgb(19, 120, 209);
    background-color: rgb(0, 0, 0);
    font-size: 34px;
    -unity-text-align: upper-center;
    background-image: none;
}

.toggle {
}

.toggle:checked {
    opacity: 0.46;
}

.container {
    flex-direction: column;
    flex-wrap: wrap;
    flex-grow: 0;
    flex-shrink: 1;
    align-items: stretch;
    justify-content: center;
    align-self: center;
    margin-top: 5px;
    margin-right: 5px;
    margin-bottom: 5px;
    margin-left: 5px;
    padding-top: 15px;
    padding-right: 15px;
    padding-bottom: 15px;
    padding-left: 15px;
    flex-basis: auto;
    width: 500px;
    background-color: rgba(48, 48, 48, 0);
    height: 50%;
    max-width: none;
}

.parent {
    flex-grow: 1;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: center;
    justify-content: center;
    align-self: center;
    width: auto;
    border-left-color: rgba(75, 75, 75, 0);
    border-right-color: rgba(75, 75, 75, 0);
    border-top-color: rgba(75, 75, 75, 0);
    border-bottom-color: rgba(75, 75, 75, 0);
    border-top-width: 10px;
    border-right-width: 10px;
    border-bottom-width: 10px;
    border-left-width: 10px;
    background-color: rgba(29, 29, 29, 0);
}
